{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start",
    "start:clean": "expo start --clear",
    "start:emulator": "adb reverse tcp:8081 tcp:8081 && set REACT_NATIVE_PACKAGER_HOSTNAME=127.0.0.1 && expo start --clear",
    "android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8082 tcp:8082 && set REACT_NATIVE_PACKAGER_HOSTNAME=127.0.0.1 && expo run:android --port 8082",
    "android:open": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8082 tcp:8082 && set REACT_NATIVE_PACKAGER_HOSTNAME=127.0.0.1 && expo run:android --no-bundler",
    "setup:mdm": "powershell -ExecutionPolicy Bypass -File ../scripts/setup-emulator-mdm.ps1",
    "test:android": "powershell -ExecutionPolicy Bypass -File ../scripts/test-android-mdm.ps1",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-firebase/app": "^21.14.0",
    "@react-native-firebase/auth": "^21.14.0",
    "@react-native-firebase/database": "^21.14.0",
    "@react-native-firebase/storage": "^21.14.0",
    "@types/styled-components": "^5.1.36",
    "events": "^3.3.0",
    "expo": "~54.0.33",
    "expo-application": "~7.0.8",
    "expo-brightness": "~14.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-intent-launcher": "~13.0.8",
    "expo-navigation-bar": "~5.0.10",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.574.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "^15.12.1",
    "react-native-view-shot": "4.0.3",
    "react-native-webview": "13.15.0",
    "socket.io-client": "^4.8.3",
    "styled-components": "^6.3.9"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~54.0.10",
    "expo-module-scripts": "^5.0.8",
    "typescript": "~5.9.2"
  },
  "private": true
}
